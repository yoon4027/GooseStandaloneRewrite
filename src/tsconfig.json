{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "outDir": "../dist",
    "composite": true,
    "tsBuildInfoFile": "../dist/tsconfig.tsbuildinfo",
    "emitDeclarationOnly": true,
    "paths": {
      "#lib/util": ["lib/util/index.ts"],
      "#lib/structures": ["lib/structures/index.ts"],
      "#types/*": ["lib/types/*"],
      "#constants/*": ["lib/constants/*"],
      "#lib/*": ["lib/*"]
    },
    "skipLibCheck": true
  },
  "include": ["."]
}
